#include <iostream>
using namespace std;

int main() {
    cout << "<<<<<Bienvenida Personita>>>>>>" << endl;

    //Aqui se declaran variables
    int numero1, numero2;

    
    cout << "Ingresa el primer número: ";
    cin >> numero1;

    cout << "Ingresa el segundo número: ";
    cin >> numero2;

    //Aqui determina si los dos numeros son iguales
    if (numero1 == numero2) {
        cout << "Muy bien,los números " << numero1 << " y " << numero2 << " son iguales." << endl;
    } else {
        cout << "Los números " << numero1 << " y " << numero2 << " no son iguales." << endl;
    }


    return 0;
}
